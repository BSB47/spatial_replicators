#!/bin/sh
qsub -P unified -pe multicore 8 -R y -v SGE_FACILITIES -l h_rt=604800,num_proc=16 -m beas -j n -o ./stdout.\$JOB_ID -e ./stderr.\$JOB_ID -b y -notify ./dnaevo -Seed 0722163033 -Display 0 -Movie 1 -MovieInt 1000 -SaveInt 1000000 -RateScale 0.892 -DiffRate 0.1 -RNADecayRate 0.02 -DNADecayRate 0.02 -MaxRNARepliRate 1. -MaxDNARepliRate 1. -MaxAssoDissRate 1. -MaxFoldUnfoldRate 1. -MaxLipidSynthRate 0. -MinRNApFoldRate 0. -InitRNApFoldRate 0. -InitRNApRNArecog 0.5 -InitRNApDNArecog 0.5 -InitDNApFoldRate 0. -InitDNApRNArecog 0. -InitDNApDNArecog 0. -InitRNApRNAdens 0.9 -InitRNApDNAdens 0. -InitDNApRNAdens 0. -InitDNApDNAdens 0. -InitParaRNAdens 0.01 -InitLipidSynthRate 0. -MutRateRecogParam 0.01 -MutStepRecogParam 0.1 -MutRateRNADNAReplRate 1e-5 -MutRateFoldUnfoldRate 0. -MutStepFoldUnfoldRate 0. -MutRateLipidSynthRate 0. -MutStepLipidSynthRate 0.1 -MutRateToPara 1e-5 -MutRateToJunk 0. -ParasiteAdv 1.1 > job_id
